		<typeAlias type="com.sigcomt.gestionProyectos.dominio.administracion.Pais" alias="Pais"/>
		<typeAlias type="com.sigcomt.gestionProyectos.dominio.administracion.Proyecto" alias="Proyecto"/>
	</typeAliases>
	
    <mappers>
        <mapper resource="mapper/usuario/UsuarioMapper.xml" />        
        <mapper resource="mapper/administracion/administracionMapper.xml" />
